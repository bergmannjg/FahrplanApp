{
    "compilerOptions": {
        "skipLibCheck": true,
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "isolatedModules": false,
        "moduleResolution": "node",
        "noEmit": false,
        "strict": true,
        "target": "ES2020",
        "resolveJsonModule": true,
        "noImplicitAny": false,
        "outDir": "dist",
        "module": "ESNext",
        "lib": [
            "ES2020"
        ],
        "sourceMap": true,
        "paths": {
            "db-prices": [
                "../../types/db-prices"
            ]
        }
    },
    "include": [
        "./*"
    ]
}