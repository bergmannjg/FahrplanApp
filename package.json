        "update-rinf-data": "rm -rf node_modules/rinf-graph/ && npm install rinf-graph",
        "android-debug": "tsc && react-native run-android --variant=debug --deviceId process.env.deviceId",
        "android-release": "tsc && react-native run-android --variant=release --deviceId process.env.deviceId",
        "android-apk-debug": "cd android && ./gradlew clean && ./gradlew :app:packageDebug",
        "android-apk-debug-install": "adb install android/app/build/outputs/apk/debug/app-debug.apk",
        "android-apk-release": "cd android && ./gradlew clean && ./gradlew :app:packageRelease",
        "android-apk-release-install": "adb install android/app/build/outputs/apk/release/app-release.apk",
        "start": "react-native start --host 0.0.0.0",
        "test": "jest",
        "tsc": "tsc",
        "lint": "eslint src --ext .ts,.tsx",
        "postinstall": "npx patch-package && npm dedupe"
    },
