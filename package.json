{
  "name": "fahrplanapp",
  "version": "1.4.0",
  "private": true,
  "scripts": {
    "update-fs-hafas-client": "rm -rf node_modules/fs-hafas-client/ && npm install fs-hafas-client",
    "update-rinf-data": "rm -rf node_modules/rinf-data/ && npm install rinf-data",
    "android-debug": "tsc && react-native run-android --variant=debug --deviceId process.env.deviceId",
    "android-release": "tsc && react-native run-android --variant=release --deviceId process.env.deviceId",
    "android-apk-release": "cd android && ./gradlew clean && ./gradlew :app:packageRelease",
    "android-apk-install": "adb install android/app/build/outputs/apk/release/app-release.apk", 
    "start": "react-native start --host 0.0.0.0",
    "test": "jest",
    "tsc": "tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "postinstall": "npx patch-package && npm dedupe"
  },
  "dependencies": {
