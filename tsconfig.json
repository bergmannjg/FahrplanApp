{
  "compilerOptions": {
    "skipLibCheck": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "es6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "es6",
    "module": "commonjs",
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "baseUrl": ".",
    "paths": {
      "react-native-simple-radio-button": [
        "types/react-native-simple-radio-button"
      ],
      "js-cache": [
        "types/js-cache"
      ],
      "react-native-image-transformer": [
        "types/react-native-image-transformer"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "types",
    "scripts",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}